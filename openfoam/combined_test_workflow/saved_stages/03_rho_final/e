FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    object      e;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 2 -2 0 0 0 0];

internalField   uniform 200000;

boundaryField
{
    inlet
    {
        type            fixedValue;
        value           uniform 200000;
    }

    outlet
    {
        type            inletOutlet;
        inletValue      uniform 200000;
        value           uniform 200000;
    }

    atmosphere
    {
        type            inletOutlet;
        inletValue      uniform 200000;
        value           uniform 200000;
    }

    rocket
    {
        type            zeroGradient;
    }
}

