FoamFile
{
    version     2.0;
    format      ascii;
    arch        "LSB;label=32;scalar=64";
    class       volVectorField;
    location    "1234";
    object      U.noSlip;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 1 -1 0 0 0 0];

internalField   uniform (0 0 500);

boundaryField
{
    inlet
    {
        type            fixedValue;
        value           uniform (0 0 500);
    }
    outlet
    {
        type            inletOutlet;
        inletValue      uniform (0 0 500);
        value           uniform (0 0 500);
    }
    atmosphere
    {
        type            slip;
    }
    rocket
    {
        type            noSlip;
    }
}


