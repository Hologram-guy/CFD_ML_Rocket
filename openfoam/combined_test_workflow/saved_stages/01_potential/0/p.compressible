FoamFile
{
    version     2.0;
    format      ascii;
    arch        "LSB;label=32;scalar=64";
    class       volScalarField;
    location    "1234";
    object      p.compressible;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [1 -1 -2 0 0 0 0];

internalField   uniform 101325;

boundaryField
{
    inlet
    {
        type            totalPressure;
        rho             rho;
        psi             none;
        gamma           1;
        p0              uniform 101325;
        value           uniform 101325;
    }
    outlet
    {
        type            zeroGradient;
    }
    atmosphere
    {
        type            waveTransmissive;
        gamma           1.4;
        fieldInf        101325;
        lInf            1;
        value           uniform 101325;
    }
    rocket
    {
        type            zeroGradient;
    }
}


